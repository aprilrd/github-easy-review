{
  "manifest_version": 2,

  "name": "Github Easy Review",
  "description": "This extension makes reviewing pull requests easier.",
  "version": "0.0.1",

  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
      {
          "matches": ["https://github.com/*/*/pull/*"],
          "js": ["jquery.js", "index.js"]
      }
  ],

  "web_accessible_resources": [
    "jquery-2.0.3.min.map"
  ]
}